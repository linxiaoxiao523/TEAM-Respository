
page {
	background: #f2f2f2;
}
.daren {
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.daren-banner-box {
	width: auto;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	height: auto;
}
.daren-banner-box .margin-tb-sm {
	margin-top: 20rpx;
	padding: 0 20rpx;
}
.daren-banner-swiper {
	width: 100%;
	height: 400rpx;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
	overflow: hidden;
	-webkit-transform: translateY(0);
	        transform: translateY(0);
}
.daren-slide-image {
	width: 100%;
	height: 260rpx;
	display: block;
	max-height: 260rpx !important;
	min-height: 260rpx !important;
	-webkit-transform: translate3d(0, 0, 0);
	        transform: translate3d(0, 0, 0);
	margin-left: 10rpx;
	margin-right: 10rpx;
	-webkit-border-top-left-radius: 12rpx;
	        border-top-left-radius: 12rpx;
	-webkit-border-top-right-radius: 12rpx;
	        border-top-right-radius: 12rpx;
}
.daren-banner-swiper .talentsub-text {
	display: block;
	background: #ffffff;
	color: #333333;
	padding: 1rem;
	font-size: 30rpx;
	-webkit-border-bottom-left-radius: 12rpx;
	        border-bottom-left-radius: 12rpx;
	-webkit-border-bottom-right-radius: 12rpx;
	        border-bottom-right-radius: 12rpx;
	margin-left: 10rpx;
	margin-right: 10rpx;
}
.talentsub-div {
	position: absolute;
	bottom: 30rpx;
	right: 20rpx;
	width: 140rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 8rpx;
	padding-top: 0;
	background: rgba(250, 250, 250, 0.9);
	-webkit-border-top-left-radius: 6px;
	        border-top-left-radius: 6px;
	-webkit-border-top-right-radius: 30px;
	        border-top-right-radius: 30px;
}
.talentsub-num {
	color: #809def;
	padding-top: 20rpx;
	padding-bottom: 10rpx;
}
.talentsub-num .number {
	text-align: center;
	font-size: 35rpx;
}
.talentsub-num .danpin {
	font-size: 30rpx;
	display: block;
	text-align: center;
}
.talentsub-div image {
	width: 120rpx;
	height: 120rpx;
	-webkit-border-radius: 6px;
	        border-radius: 6px;
}
.talentsub-background {
	position: absolute;
	top: 0;
	width: 100%;
	height: 140px;
	background-image: url('http://img.fqapps.com/FqkeWFSpNCpKEiM6Qgm_sabSD5Ag');
}
.week-title {
	font-size: 36rpx;
	padding-left: 38rpx;
	padding-top: 0rpx;
	padding-bottom: 10rpx;
	color: #484848;
}
.week-new {
	width: 100%;
	max-width: 100%;
}
.week-container {
	white-space: nowrap;
	margin: 0 auto;
	position: relative;
	overflow: hidden;
	list-style: none;
	padding: 0;
	z-index: 1;
	padding-top: 0rpx;
	padding-left: 25rpx;
	padding-right: 25rpx;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
}
.week-container .swiper-wrapper {
	position: relative;
	width: 100%;
	height: 100%;
	z-index: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-transition-property: -webkit-transform;
	transition-property: -webkit-transform;
	transition-property: transform;
	transition-property: transform, -webkit-transform;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
	-webkit-transition-timing-function: ease-out;
	        transition-timing-function: ease-out;
	margin: 0 auto;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
}
.week-container .swiper-wrapper .new-article {
	display: inline-block;
	background: #ffffff;
	-webkit-border-radius: 12rpx;
	        border-radius: 12rpx;
	-webkit-flex-shrink: 0;
	flex-shrink: 0;
	width: 70%;
	height: 100%;
	position: relative;
	-webkit-transition-property: -webkit-transform;
	transition-property: -webkit-transform;
	transition-property: transform;
	transition-property: transform, -webkit-transform;
	-webkit-transform: translate3d(0, 0, 0);
	        transform: translate3d(0, 0, 0);
	margin-right: 20rpx;
}
.week-container .swiper-wrapper .new-article .article-img {
	width: 100%;
	max-height: 300rpx;
	display: block;
	-webkit-border-top-left-radius: 12rpx;
	        border-top-left-radius: 12rpx;
	-webkit-border-top-right-radius: 12rpx;
	        border-top-right-radius: 12rpx;
}
.week-container .swiper-wrapper .new-article .article-title {
	font-size: 30rpx;
	color: #484848;
	display: block;
	padding-top: 16rpx;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	padding-left: 16rpx;
}
.week-container .swiper-wrapper .new-article .article-info {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 16rpx;
}
.week-container .swiper-wrapper .new-article .info-img {
	width: 45rpx;
	height: 45rpx;
	-webkit-border-radius: 100%;
	        border-radius: 100%;
	margin-right: 16rpx;
}
.week-container .swiper-wrapper .new-article .info-name {
	font-size: 30rpx;
	color: #666;
}
.week-container .swiper-wrapper .new-article .detail {
	position: absolute;
	right: 16rpx;
	font-size: 26rpx;
	color: #666;
}

/*大家都在逛*/
